package com.yile.games.doll.net.vo
{
    /*@import*/
    /**
     * 竞技场排行信息
     */
    public class PKRankInfo
    {
        /**
         * 用户ID
         */
        public var userID:uint;

        /**
         * 姓名
         */
        public var nickName:String;

        /**
         * 连胜次数
         */
        public var value:uint;

        /**
         * 本次排名
         */
        public var rank:uint;

    }
}