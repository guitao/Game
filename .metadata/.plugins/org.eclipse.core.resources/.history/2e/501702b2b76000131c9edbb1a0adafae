package com.yile.games.doll.net.response.scene
{
    import com.yile.framework.net.IResponse;
    import com.yile.games.doll.net.vo.*;
    import com.yile.framework.net.Packet;

    /*@import*/

    /**
     * 攻击(5:38)
     */
    public class AttackStartResponse implements IResponse
    {
        /**
         * 结果 0ok 1禁止攻击
         */
        public var ret:uint;

        /**
         * 出击宠物ID
         */
        public var baseID:uint;

        public function read(p:Packet):void
        {

            ret = p.readUnsignedByte();
            baseID = p.readUnsignedInt();
        }
    }
}