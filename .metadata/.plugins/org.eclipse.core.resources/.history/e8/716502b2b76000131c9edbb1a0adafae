package com.yile.games.doll.net.response.scene
{
    import com.yile.framework.net.IResponse;
    import com.yile.games.doll.net.vo.*;
    import com.yile.framework.net.Packet;

    /*@import*/

    /**
     * 批量移除人物（宠物）(5:16)
     */
    public class BatchRemoveMapDataResponse implements IResponse
    {
        /**
         * 数量
         */
        public var num:uint;

        /**
         * 数据唯一ID
         */
        public var infoList:Array /* of GUID */;

        public function read(p:Packet):void
        {

            num = p.readUnsignedShort();
			infoList = [];
            for(var i:int = 0; i < num; i++){
                var gUID:GUID = new GUID();
                gUID.id = p.readUnsignedInt();
                gUID.type = p.readUnsignedByte();
                infoList.push(gUID);
            }
        }
    }
}