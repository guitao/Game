package com.yile.games.doll.net.response.quest
{
    import com.yile.framework.net.IResponse;
    import com.yile.games.doll.net.vo.*;
    import com.yile.framework.net.Packet;

    /*@import*/

    /**
     * 结束任务(12:11)
     */
    public class QuestFinishResponse implements IResponse
    {
        /**
         * 0成功 1已递交 2失败
         */
        public var ret:uint;

        /**
         * 任务ID
         */
        public var id:uint;

        public function read(p:Packet):void
        {

            ret = p.readUnsignedByte();
            id = p.readUnsignedInt();
        }
    }
}