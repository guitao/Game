package com.yile.games.doll.net.response.home
{
    import com.yile.framework.net.IResponse;
    import com.yile.games.doll.net.vo.*;
    import com.yile.framework.net.Packet;

    /*@import*/

    /**
     * 拾取魂精返回(15:28)
     */
    public class PickUpSoulResponse implements IResponse
    {
        /**
         * 0成功 1ID未发现
         */
        public var ret:uint;

        /**
         * ID
         */
        public var id:uint;

        public function read(p:Packet):void
        {

            ret = p.readUnsignedByte();
            id = p.readUnsignedShort();
        }
    }
}