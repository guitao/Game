package com.yile.games.doll.net.vo
{
    /*@import*/
    /**
     * 祝福奖励信息
     */
    public class GodLikeReward
    {
        /**
         * 奖励ID
         */
        public var id:uint;

        /**
         * 领取状态 0不可领取 1可领取 2领取过
         */
        public var state:uint;

        /**
         * 礼包ID
         */
        public var reward:uint;

    }
}