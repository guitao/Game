package com.yile.games.doll.net.request.home
{
    import com.yile.framework.net.IRequest;
    import com.yile.games.doll.net.vo.*;
    import com.yile.framework.net.Packet;

    /**
     * 进入家园(15:1)
     */
    public class EnterHomeRequest implements IRequest
    {
        /**
         * 家园ID，同玩家ID
         */
        public var id:uint;
    
        public function get module():uint
        {
            return 15;
        }

        public function get action():uint
        {
            return 1;
        }

        public function write(p:Packet):void
        {
            
            p.writeUnsignedInt(id);
        }
    }
}