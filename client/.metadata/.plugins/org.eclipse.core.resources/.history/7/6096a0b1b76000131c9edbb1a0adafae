package com.yile.games.doll.net.vo
{
    /*@import*/
    /**
     * 配置技能
     */
    public class ConfigSkillInfo
    {
        /**
         * 主要技
         */
        public var mainSkill:uint;

        /**
         * 普通技
         */
        public var normalSkill:uint;

        /**
         * 特殊技
         */
        public var specialSkill:uint;

        /**
         * 绝技
         */
        public var greatSkill:uint;

    }
}