package com.yile.games.doll.net.vo
{
    /*@import*/
    /**
     * 神像信息
     */
    public class GodLikeInfo
    {
        /**
         * 状态 0正常 1祝福满 2冷却中
         */
        public var state:uint;

        /**
         * 等级
         */
        public var level:uint;

        /**
         * 祝福次数
         */
        public var blessCount:uint;

    }
}