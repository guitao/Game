package stock.com.ui.elements.xpainting.note
{
    import flash.text.*;

    public class IconNote extends TextField
    {

        public function IconNote(param1:String)
        {
            this.init(param1);
            return;
        }// end function

        private function init(param1:String) : void
        {
            super.border = true;
            super.text = param1;
            super.autoSize = "center";
            super.background = true;
            super.backgroundColor = 16645833;
            return;
        }// end function

    }
}
