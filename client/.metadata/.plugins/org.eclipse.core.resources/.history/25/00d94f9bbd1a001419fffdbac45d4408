package com.ui
{
    import hsuan.*;

    public class SetFloaterUi extends SetUiBase
    {

        public function SetFloaterUi()
        {
            _panel.x = 88;
            _panel.y = 50;
            addOption("默认打开K线指示浮层", _cfg.FloaterSo);
            addOption("默认关闭K线指示浮层", !_cfg.FloaterSo);
            return;
        }// end function

        override protected function toSave() : void
        {
            var _loc_1:* = SingleOption(_opArr[0]).s;
            if (_so)
            {
                _so.data.showFloater = _loc_1;
            }
            var _loc_2:* = _loc_1;
            _cfg.showFloater = _loc_1;
            _cfg.FloaterSo = _loc_2;
            showNotice();
            return;
        }// end function

    }
}
