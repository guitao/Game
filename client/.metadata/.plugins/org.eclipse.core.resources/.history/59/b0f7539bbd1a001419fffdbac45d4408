        }// end function

        private function statisticDone(event:Event) : void
        {
            event.target.removeEventListener(Event.COMPLETE, this.statisticDone);
            event.target.removeEventListener(IOErrorEvent.IO_ERROR, this.statisticDone);
            event.target.removeEventListener(SecurityErrorEvent.SECURITY_ERROR, this.statisticDone);
            return;
        }// end function

