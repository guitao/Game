        public function mainTabStatistic(param1:int, param2:Boolean = false) : void
        {
            var _loc_3:String = null;
            switch(param1)
            {
                case UrlHashDecoder.TIME_SHARE_ID:
                {
                    if (param2)
                    {
                        return;
                    }
                }
                case UrlHashDecoder.FAKE_T5DAY_ID:
                case UrlHashDecoder.CLOSE_LINE_ID:
                {
                    _loc_3 = "fenshi.html";
                    if (param1 == UrlHashDecoder.FAKE_T5DAY_ID)
                    {
                        _loc_3 = _loc_3 + "?type=5day";
                    }
                    else if (param1 == UrlHashDecoder.CLOSE_LINE_ID)
                    {
                        _loc_3 = _loc_3 + "?type=1year";
                    }
                    break;
                }
                case UrlHashDecoder.DAY_K_ID:
                case UrlHashDecoder.WEEK_K_ID:
                case UrlHashDecoder.MONTH_K_ID:
                {
                    _loc_3 = "kline.html?type=";
                    if (param1 == UrlHashDecoder.WEEK_K_ID)
                    {
                        _loc_3 = _loc_3 + "week";
                    }
                    else if (param1 == UrlHashDecoder.MONTH_K_ID)
                    {
                        _loc_3 = _loc_3 + "month";
                    }
                    else
                    {
                        _loc_3 = _loc_3 + "day";
                    }
                    _loc_3 = _loc_3 + ("&re=" + this.reK);
                    break;
                }
                case UrlHashDecoder.MIN_K_5_ID:
                case UrlHashDecoder.MIN_K_15_ID:
                case UrlHashDecoder.MIN_K_30_ID:
                case UrlHashDecoder.MIN_K_60_ID:
                {
                    _loc_3 = "minkline.html?type=";
                    if (param1 == UrlHashDecoder.MIN_K_5_ID)
                    {
                        _loc_3 = _loc_3 + "5min";
                    }
                    else if (param1 == UrlHashDecoder.MIN_K_15_ID)
                    {
                        _loc_3 = _loc_3 + "15min";
                    }
                    else if (param1 == UrlHashDecoder.MIN_K_30_ID)
                    {
                        _loc_3 = _loc_3 + "30min";
                    }
                    else
                    {
                        _loc_3 = _loc_3 + "60min";
                    }
                    break;
                }
                default:
                {
                    break;
                }
            }
            if (_loc_3)
            {
                this.sendStatistic(_loc_3);
            }
            return;
        }// end function

