        public function sendStatistic(param1:String) : void
        {
            param1 = "http://finance.sina.com.cn/stat/flash/" + param1;
            var _loc_2:* = new URLLoader();
            _loc_2.addEventListener(IOErrorEvent.IO_ERROR, this.statisticDone);
            _loc_2.addEventListener(SecurityErrorEvent.SECURITY_ERROR, this.statisticDone);
            _loc_2.addEventListener(Event.COMPLETE, this.statisticDone);
            _loc_2.load(new URLRequest(param1));
            return;
        }// end function

