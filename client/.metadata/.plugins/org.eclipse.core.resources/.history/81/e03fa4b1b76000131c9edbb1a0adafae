package com.yile.games.doll.net.vo
{
    /*@import*/
    /**
     * 消耗品BUF
     */
    public class LeechdomInfo
    {
        /**
         * 效果ID(技能效果)
         */
        public var effect:uint;

        /**
         * 影响值
         */
        public var value:uint;

        /**
         * 剩余次数限制
         */
        public var num:uint;

        /**
         * 使用时间
         */
        public var startTime:uint;

        /**
         * 结束时间
         */
        public var dueTime:uint;

    }
}