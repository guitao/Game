package com.sina.finance.data
{

    public class DataObjBase extends Object
    {
        public var labelMaxP:Number;
        protected var _dataTool:DataTool;
        public var maxPrice:Number;
        public var maxPercent:Number;
        public var hq:HQ;
        public var marks:Array;
        public var datasLength:uint;
        public var labelVolumeCount:uint;
        public var minPercent:Number;
        public var dates:Array;
        public var minPrice:Number;
        public var datas:Array;
        public var maxVolume:Number;
        public var labelPriceCount:uint;
        public var labelMaxVol:Number;
        protected var _cfg:Config;
        public var labelMinP:Number;
        public var prevclose:Number;
        public var days:uint = 1;
        public var poses:Array;

        public function DataObjBase(param1:HQ)
        {
            this._cfg = Config.getInstance();
            this._dataTool = DataTool.getInstance();
            this.hq = param1;
            return;
        }// end function

        protected function setExtValue() : void
        {
            return;
        }// end function

        protected function initExtValue() : void
        {
            this.minPrice = 1000000;
            this.maxPrice = 0.01;
            this.minPercent = -0.0009;
            this.maxPercent = 0.001;
            this.maxVolume = 1;
            return;
        }// end function

        protected function calcPoses() : void
        {
            return;
        }// end function

        protected function initPoses() : void
        {
            return;
        }// end function

    }
}
