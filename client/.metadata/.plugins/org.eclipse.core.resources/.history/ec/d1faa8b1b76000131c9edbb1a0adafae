package com.yile.games.doll.net.vo
{
    /*@import*/
    /**
     * 物品信息
     */
    public class SimpleItemInfo
    {
        /**
         * 物品唯一id
         */
        public var id:uint;

        /**
         * 物品类别id
         */
        public var baseID:uint;

        /**
         * 位置
         */
        public var pos:ItemLocationInfo;

        /**
         * 数量
         */
        public var num:uint;

    }
}