package stock.com.events
{
    import flash.events.*;
    import stock.com.ui.elements.xpainting.icon.*;

    public class XPaintingEvent extends Event
    {
        public var icon:IconBase = null;
        public static const ICONSELECTED:String = "iconSelected";
        public static const ENDPAINTER:String = "endPainter";

        public function XPaintingEvent(param1:String, param2:Boolean = false, param3:Boolean = false)
        {
            super(param1, param2, param3);
            return;
        }// end function

        override public function toString() : String
        {
            return formatToString("XPaintingEvent", "type", "bubbles", "cancelable", "eventPhase");
        }// end function

        override public function clone() : Event
        {
            return new XPaintingEvent(type, bubbles, cancelable);
        }// end function

    }
}
