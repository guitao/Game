package com.yile.games.doll.net.vo
{
    /*@import*/
    /**
     * 申请列表信息
     */
    public class ApplyInfo
    {
        /**
         * 用户ID
         */
        public var userID:uint;

        /**
         * 用户名称
         */
        public var nickName:String;

        /**
         * 用户性别
         */
        public var sex:uint;

        /**
         * 用户等级
         */
        public var level:uint;

        /**
         * 战斗力
         */
        public var combat:uint;

        /**
         * 申请时间
         */
        public var applyTime:uint;

    }
}