package com.yile.games.doll.net.response.user
{
    import com.yile.framework.net.IResponse;
    import com.yile.games.doll.net.vo.*;
    import com.yile.framework.net.Packet;

    /*@import*/

    /**
     * 请求领取首次充值礼包信息(2:124)
     */
    public class FirstRechargeRewardInfoResponse implements IResponse
    {
        /**
         * 领取返回 0成功 1已领取 2不能领取 3错误
         */
        public var ret:uint;

        public function read(p:Packet):void
        {

            ret = p.readUnsignedByte();
        }
    }
}